      fixedlength
    }
  }
  channel "scripted"
  {
    event sequence "throw"
    {
      time 0.006666 2.840000
      ramp 1.00 1.00 1.00
      param "throwshotgun"
      fixedlength
    }
  }
}

mapname "maps\balcony_large.bsp"
